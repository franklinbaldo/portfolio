  "bugs": {
    "url": "https://github.com/franklinbaldo/portfolio-manager/issues"
  },
  "homepage": "https://github.com/franklinbaldo/portfolio-manager#readme",
  "devDependencies": {
    "simple-git": "^3.20.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
