    "check-status": "node scripts/check-status.js",
    "cleanup": "node scripts/cleanup.js",
    "backup": "node scripts/backup.js",
    "start": "npm run check-status"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/franklinbaldo/portfolio-manager.git"
  },
  "keywords": [
    "portfolio",
    "repository-management",
    "github",
    "legal-tech",
    "ai-ml",
    "automation",
    "project-management",
    "git-tools"
  ],
  "author": "Franklin Baldo <franklinbaldo@gmail.com>",
